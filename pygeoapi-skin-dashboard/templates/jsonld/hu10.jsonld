{
    "@context": {
        "schema": "https://schema.org/",
        "gsp": "http://www.opengis.net/ont/geosparql#",
        "hyf": "https://www.opengis.net/def/schema/hy_features/hyf/",
        "gnis_url": {
            "@id": "schema:subjectOf",
            "@type": "@id"
        }
    },
    "@id": "{{ data['@id'] }}",
    "@type": "schema:Place",
    "schema:name": "Little Sandy Creek",
    "gnis_url": "{{ data['gnis_url'] }}",
    "hyf:containingCatchment": ["https://geoconnex.us/ref/hu08/{{ data['id'][0:8] }}","https://geoconnex.us/ref/hu06/{{ data['id'][0:6] }}","https://geoconnex.us/ref/hu04/{{ data['id'][0:4] }}","https://geoconnex.us/ref/hu02/{{ data['id'][0:2] }}"],
    "gsp:hasGeometry": {{ data['gsp:hasGeometry'] | to_json }},
    "geo": {{ data['schema:geo'] | to_json | safe }}
}
